{
    "version": "0.2.0",
    "inputs": [
        {
            "id": "launch_app",
            "type": "pickString",
            "default": "app",
            "options": [
                "app",
                "sdk",
                "plugin/infcurion_plugin/example"
            ],
            "description": "実行するappを選択(app/sdk)"
        }
    ],
    "configurations": [
        {
            "name": "debug",
            "cwd": "${input:launch_app}",
            "request": "launch",
            "type": "dart"
        },
        {
            "name": "profile",
            "cwd": "${input:launch_app}",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile"
        },
        {
            "name": "release",
            "cwd": "${input:launch_app}",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release"
        }
    ]
}